{
	"name": "gulp-skip-file",
	"version": "0.1.1",
	"description": "Gulp plugin to skip files.",
	"main": "index.js",
	"scripts": {
		"test": "xo && nyc ava"
	},
	"engines": {
		"node": ">=6"
	},
	"author": "Corey Farrell",
	"license": "MIT",
	"files": [
		"fixtures",
		"*.js"
	],
	"keywords": [
		"gulp",
		"skip",
		"ignore"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/cfware/gulp-skip-file.git"
	},
	"bugs": {
		"url": "https://github.com/cfware/gulp-skip-file/issues"
	},
	"homepage": "https://github.com/cfware/gulp-skip-file#readme",
	"dependencies": {
		"through2": "^3.0.0"
	},
	"devDependencies": {
		"ava": "^1.0.1",
		"gulp": "^4.0.0",
		"gulp-if": "^2.0.2",
		"nyc": "^12.0.2",
		"xo": "^0.21.1"
	},
	"nyc": {
		"check-coverage": true,
		"per-file": true,
		"lines": 100,
		"statements": 100,
		"functions": 100,
		"branches": 100,
		"include": [
			"*.js",
			"!test.js"
		]
	},
	"xo": {
		"rules": {
			"comma-dangle": [
				"error",
				"always-multiline"
			]
		}
	}
}
